{
  "name": "@cmdcode/core-cmd",
  "version": "1.0.0",
  "description": "Command Bitcoin Core using a suite of automation tools.",
  "author": "Christopher Scott",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmdruid/core-cmd.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/*",
    "release": "yarn clean && yarn build",
    "scratch": "tsx test/scratch.ts",
    "test": "tsx test/tape.ts"
  },
  "dependencies": {
    "@cmdcode/crypto-utils": "^2.3.9",
    "@scrow/tapscript": "^2.0.2",
    "@types/node": "^18.13.0"
  },
  "devDependencies": {
    "@types/tape": "^5.6.0",
    "tape": "^5.6.6",
    "tslib": "^2.6.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}

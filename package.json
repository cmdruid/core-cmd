{
  "name": "@cmdcode/core-cmd",
  "version": "1.5.7",
  "description": "Command Bitcoin Core using a suite of automation tools.",
  "author": "Christopher Scott",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmdruid/core-cmd.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/*",
    "release": "yarn clean && yarn test && yarn build",
    "scratch": "tsx test/scratch.ts",
    "test": "tsx test/tape.ts"
  },
  "dependencies": {
    "@cmdcode/buff": "^2.2.5",
    "@cmdcode/crypto-tools": "^2.7.6",
    "@cmdcode/psbt2": "^0.0.8",
    "@scrow/tapscript": "^2.2.2",
    "@types/node": "^20.11.10"
  },
  "devDependencies": {
    "@types/tape": "^5.6.4",
    "tape": "^5.7.4",
    "tslib": "^2.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
